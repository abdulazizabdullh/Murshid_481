{
  "buildCommand": "cd Murshid-Frontend && npm run build",
  "outputDirectory": "Murshid-Frontend/dist",
  "installCommand": "cd Murshid-Frontend && npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/((?!assets/).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
